<!DOCTYPE html>
<html>
  <head>
    <title>SWEXEL3</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
  
    <div id="container">
        <div id="header">
          <%= render "header" %>
        </div>
        <div id="sidebar">
          <%= render "sidebar" %>
        </div>
        <div id="content">
          <%= yield %>
        </div>
  </body>
</html>
